		// options
		"model_option0_text" := "HF1";
		"model_option0_yesno" := "no";
		"model_option1_text" := "HF2";
		"model_option1_yesno" := "no";
		"model_option3_text" := "MAC";
		"model_option3_yesno" := "no";
		// wide distance
		"token_distance_wide" := "17";
		// font
